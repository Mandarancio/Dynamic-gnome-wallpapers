<background>
  <starttime>
    <year>2011</year>
    <month>11</month>
    <day>24</day>
    <hour>7</hour>
    <minute>00</minute>
    <second>00</second>
  </starttime>

<!-- This animation will start at 7 AM. -->

<!-- We start with sunrise at 7 AM. It will remain up for 10 hour. -->
<static>
<duration>36000.0</duration>
<file>/usr/share/backgrounds/gnome/Minimal-Mojave-timed/Minimal-Mojave-day.jpeg</file>
</static>

<!-- Sunrise starts to transition to day at 5 PM. The transition lasts for 2 hours, ending at 7 PM. -->
<transition type="overlay">
<duration>7200.0</duration>
<from>/usr/share/backgrounds/gnome/Minimal-Mojave-timed/Minimal-Mojave-day.jpeg</from>
<to>/usr/share/backgrounds/gnome/Minimal-Mojave-timed/Minimal-Mojave-night.jpeg</to>
</transition>

<!-- We start with sunrise at 7 PM. It will remain up for 10 hour. -->
<static>
<duration>36000.0</duration>
<file>/usr/share/backgrounds/gnome/Minimal-Mojave-timed/Minimal-Mojave-night.jpeg</file>
</static>

<!-- Sunrise starts to transition to day at 5 AM. The transition lasts for 2 hours, ending at 7 AM. -->
<transition type="overlay">
<duration>7200.0</duration>
<from>/usr/share/backgrounds/gnome/Minimal-Mojave-timed/Minimal-Mojave-night.jpeg</from>
<to>/usr/share/backgrounds/gnome/Minimal-Mojave-timed/Minimal-Mojave-day.jpeg</to>
</transition>

</background>
